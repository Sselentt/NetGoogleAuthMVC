@model List<Microsoft.AspNetCore.Authentication.AuthenticationScheme>
@{
    ViewData["Title"] = "Home Page";
}

    <form method="post" asp-action="GoogleLogin" asp-controller="Home">
        <div>
            @foreach (var provider in Model)
            {
                <button type="submit" class="btn btn-primary" name="provider" value="@provider.Name">@provider.DisplayName</button>
            }
        </div>
    </form>
